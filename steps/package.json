{
  "name": "steps",
  "version": "1.0.0",
  "description": "workshop for modern javascript school",
  "steps": [
    "bases",
    "bases-solution",
    "promises",
    "promises-solution",
    "classes",
    "classes-solution",
    "proxies",
    "proxies-solution",
    "generators",
    "generators-solution",
    "modules",
    "modules-solution",
    "functional",
    "functional-solution",
    "redux",
    "redux-solution",
    "typescript",
    "typescript-solution"
  ],
  "scripts": {
    "test": "jest --config=\"jest.config.js\" --watch",
    "bases": "jest steps/bases/test.spec.js",
    "bases-solution": "jest steps/bases-solution/test.spec.js",
    "promises": "jest steps/promises/test.spec.js",
    "promises-solution": "jest steps/promises-solution/test.spec.js",
    "classes": "jest steps/classes/test.spec.js",
    "classes-solution": "jest steps/classes-solution/test.spec.js",
    "proxies": "jest steps/proxies/test.spec.js",
    "proxies-solution": "jest steps/proxies-solution/test.spec.js",
    "generators": "jest steps/generators/test.spec.js",
    "generators-solution": "jest steps/generators-solution/test.spec.js",
    "modules": "rollup modules/module-A.js -o modules/bundle.js",
    "modules-solution": "rollup modules-solution/module-A.js -o modules-solution/bundle.js",
    "functional": "jest steps/functional/test.spec.js",
    "functional-solution": "jest steps/functional-solution/test.spec.js",
    "redux": "serve ./redux",
    "redux-solution": "serve ./redux-solution",
    "typescript": "tsc",
    "typescript-solution": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sfeir-open-source/sfeir-school-modern-javascript.git"
  },
  "dependencies": {
    "axios": "^0.30.0",
    "jest": "^29.7.0",
    "ramda": "^0.30.1",
    "rollup": "^4.40.0",
    "ts-jest": "^29.3.2",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "prettier": "^3.5.3",
    "rxjs": "^6.6.7",
    "serve": "^14.2.4"
  }
}
